.uib-typeahead
  position absolute
  left 0
  right 0
  top 100%
  display block
  min-width 120px
  margin 8px 0 0
  padding 0
  list-style none
  box-shadow 1px 2px 3px 2px rgba(0,0,0,0.3)
  background #fff
  color #4f4e57
  z-index 10
  li a
    display block
    padding 4px 8px
    text-decoration none
    transition .2s
    &:hover
      background rgba(#efefef, .5)
  li.active a
    background #efefef
